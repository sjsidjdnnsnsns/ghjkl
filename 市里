end

local function onPlayerAdded(player)
    if isPlayerWhitelisted(player) then
        Notification:Notify(
            {Title = "SansHub", Description = player.Name .. " 刚刚进入他是同脚本拥有者"},
            {OutlineColor = Color3.fromRGB(250, 80, 80), Time = 5, Type = "image"},
            {Image = "http://www.roblox.com/asset/?id=139537085184495", ImageColor = Color3.fromRGB(255, 84, 84)}
        )
    end
end

local function checkAllPlayers()
    for _, player in ipairs(game.Players:GetPlayers()) do
        if isPlayerWhitelisted(player) then
            Notification:Notify(
                {Title = "SansHub", Description = player.Name .. " 是同脚本拥有者"},
                {OutlineColor = Color3.fromRGB(250, 80, 80), Time = 5, Type = "image"},
                {Image = "http://www.roblox.com/asset/?id=139537085184495", ImageColor = Color3.fromRGB(255, 84, 84)}
            )
        end
    end
end

game.Players.PlayerAdded:Connect(onPlayerAdded)


checkAllPlayers()

checkPlayer()

